{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        // "plugin:react/recommended",
        // "google",
        "eslint:recommended",
        "plugin:jest/recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react","jest"
    ],
    "rules": {
    },

    "overrides": [
      {
        "files": [
          "**/*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  }
